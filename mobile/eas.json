{
  "cli": {
    "version": ">= 15.0.12",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_DEV_FLAG": "true" ,
        "EXPO_PUBLIC_BACKEND_URL": "this_should_never_be_used" ,
        "EXPO_PUBLIC_SENSOR_DATA_URL": "this_should_never_be_used"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_DEV_FLAG": "false" ,
        "EXPO_PUBLIC_BACKEND_URL": "${EXPO_PUBLIC_BACKEND_URL}",
        "EXPO_PUBLIC_SENSOR_DATA_URL": "${EXPO_PUBLIC_SENSOR_DATA_URL}",
        "EXPO_PUBLIC_GOOGLE_API_KEY": "${EXPO_PUBLIC_GOOGLE_API_KEY}"
      }
    }
  },
  "submit": {
    "production": {

    }
  }
}
